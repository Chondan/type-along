{
  "manifest_version": 3,
  "name": "Type Along",
  "version": "1.0",
  "description": "Practice typing while reading any webpage.",
  "permissions": ["scripting"],
  "action": {
    "default_title": "Start Type Along",
    "default_icon": "icons/icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["style.css"]
    }
  ]
}
